<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<HBox fx:id="container" prefHeight="800.0" prefWidth="1200.0" spacing="10.0" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.aber.adk15.controllers.ui.ApplicationViewController">
    <VBox fx:id="menu" spacing="10.0" HBox.hgrow="NEVER">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <Button fx:id="startButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#start" text="Start" VBox.vgrow="ALWAYS">
         <font>
            <Font name="Meera" size="13.0" />
         </font></Button>
        <Button fx:id="loadCreasePatternButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#loadCreasePattern" text="Load Crease Pattern" VBox.vgrow="ALWAYS">
         <font>
            <Font name="Meera" size="13.0" />
         </font></Button>
        <Button fx:id="configureButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#configure" text="Configure" VBox.vgrow="ALWAYS">
         <font>
            <Font name="Meera" size="13.0" />
         </font></Button>
        <Label fx:id="loadedCreasePatternLabel" text="No Crease Pattern Loaded" textAlignment="CENTER" VBox.vgrow="ALWAYS" />
    </VBox>
</HBox>
